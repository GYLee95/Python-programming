def solution(s):
    for i in range(len(s)):
      found = True
      for j in range(i+1, len(s)):
        if s[i]==s[j]:
          found = False
          break
      if found==True:
        return i
    return -1

if __name__ == '__main__':
    print(solution('alphabet'))
    print(solution('barbados'))
    print(solution('crunchy'))
